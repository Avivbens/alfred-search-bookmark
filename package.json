{
    "name": "alfred-search-bookmark",
    "version": "1.0.4",
    "description": "Alfred Workflow to search bookmarks in Chrome, support multi-profile",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "rm -rf dist && tsc-esm",
        "start": "node dist/index.js",
        "postinstall": "alfy-init && npm run build",
        "preuninstall": "alfy-cleanup",
        "pre-commit": "pretty-quick --staged && npm run build && git add dist",
        "prepare": "husky install || exit 0"
    },
    "keywords": [
        "alfred",
        "workflow",
        "bookmark",
        "chrome",
        "search",
        "multi-profile"
    ],
    "author": {
        "name": "Aviv Ben Shahar",
        "email": "avivbens87@gmail.com"
    },
    "publisher": "Aviv Ben Shahar",
    "homepage": "https://github.com/Avivbens/alfred-search-bookmark",
    "license": "ISC",
    "dependencies": {
        "alfy": "^1.1.0"
    },
    "devDependencies": {
        "@digitak/tsc-esm": "^3.1.4",
        "@types/node": "^18.11.18",
        "husky": "^8.0.3",
        "prettier": "^2.8.3",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.9.4"
    }
}
